{
  "name": "easy-terminal-repo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npx tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint",
    "demo": "cd ./demo && npm install",
    "pre-publish": "npm run compile && npm run copyfiles && npm run minify",
    "copyfiles": "copyfiles  dist/index* build -f -a && copyfiles dist/models/*.ts build/models -f && copyfiles readme.md build",
    "minify": "terser --compress --mangle --output ./build/index.js -- ./build/index.js"
  },
  "author": ".",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.11.2",
    "gts": "^3.1.1",
    "terser": "^5.16.0",
    "typescript": "~4.7.0",
    "copyfiles": "^2.4.1"
  }
}